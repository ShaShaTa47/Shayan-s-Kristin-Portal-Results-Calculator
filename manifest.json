{
    "name": "Shayan's Kristin-Portal-Results-Calculator",
    "key": "",
    "icons": {
        "16": "IconFolderGoogle/kristinicon.png",
        "48": "IconFolderGoogle/kristinicon.png",
        "128": "IconFolderGoogle/kristinicon.png"
    },
    "description": "An extension for the use of https://portal.kristin.school.nz/group/portal/my-info/results",
    "manifest_version": 3,
    "version": "1.0",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "nativeMessaging"
    ],
    "host_permissions": [
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "https://portal.kristin.school.nz/*"
    ],
    "content_scripts": [
        {
            "all_frames": true,
            "matches": [
                "https://portal.kristin.school.nz/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}