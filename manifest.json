{
    "name": "MYP Statistical Calculator for Kristin Portal",
    "icons": {
        "128": "IconFolder/128.png",
        "48": "IconFolder/48.png",
        "16": "IconFolder/16.png"
    },
    "description": "An extension for the use of https://portal.kristin.school.nz/group/portal/my-info/results",
    "manifest_version": 3,
    "version": "1.6",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "all_frames": true,
            "matches": [
                "https://portal.kristin.school.nz/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}